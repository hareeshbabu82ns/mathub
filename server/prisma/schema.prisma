generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model PushSubscription {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  endpoint String  @unique
  topic    String? @default("default")
  data     Json
}

